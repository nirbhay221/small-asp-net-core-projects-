@model EmpManagement.Models.Complete
    <div class="row">

        <a asp-controller="mainhome" asp-action="newindex">Index</a>
        <div class="col-xs-4">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <a asp-controller="UpNext" asp-action="index">UpNext</a>
                </div>
                <div class="panel-body">
                    <div id="one">
                        &nbsp;

                        @foreach (var item in ViewBag.UpNext)
                        {
                            <div class="alert alert-success" role="alert">@item.Name @item.Status</div>
                            <a asp-controller="KanbanViewProject" asp-action="arrowone" asp-route-id="@item.Id" onclick="return confirm('Are you sure you want to move ?')"><span class="glyphicon glyphicon-menu-left" aria-hidden="true"></span></a>
